<h2>{{header}}</h2><br>
<div class="products">
  {% for file in files %}
    <div class="products-item">
      <h2 class="head">Картинка</h2>
      <div class="imgbox">
        <a href='?file={{ file }}' target='_self'>
          <img src='{{ images}}{{ file }}' alt='product' class="img">
        </a>
      </div>
    </div>
  {% endfor %}
</div>    
